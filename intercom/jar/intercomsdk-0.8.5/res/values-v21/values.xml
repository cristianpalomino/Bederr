<?xml version="1.0" encoding="utf-8"?>
<resources>
     
    <eat-comment/>
    <style name="IntercomSDK_EditText" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/intercomsdk_themed_edit_text</item>
    </style>
    <style name="IntercomSDK_LightTheme" parent="android:Theme.Material">
        <item name="android:editTextStyle">@style/IntercomSDK_EditText</item>
    </style>
    <style name="IntercomSDK_LightTheme.Dialog" parent="android:Theme.Material.Light.Dialog">
        <item name="android:editTextStyle">@style/IntercomSDK_EditText</item>
    </style>
    <style name="IntercomSDK_MyAnimation">
        <item name="android:windowEnterAnimation">@null</item>
        <item name="android:windowExitAnimation">@null</item>
    </style>
    <style name="IntercomSDK_PanelTheme" parent="@android:style/Theme.Material.Light.Panel">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/IntercomSDK_MyAnimation</item>
    </style>
</resources>